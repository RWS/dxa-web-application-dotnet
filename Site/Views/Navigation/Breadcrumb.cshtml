@model Sdl.Web.Mvc.Models.Breadcrumb
@if (Model.Items!=null && Model.Items.Count>0)
{
<ol class="breadcrumb">
    <li>
       <a href="@Model.Items[0].Url"><i class="fa fa-home"><span class='sr-only'>@Model.Items[0].LinkText</span></i></a>
    </li>
    @for(int i=1;i<Model.Items.Count-1;i++)
    {
        <li><a href="@Model.Items[i].Url">@Model.Items[i].LinkText</a></li>
    }
    @if (Model.Items.Count>1)
    {      
        <li class="active">@Model.Items[Model.Items.Count-1].LinkText</li>
    }
</ol>
}



